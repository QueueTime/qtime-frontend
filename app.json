{
  "expo": {
    "name": "QTime",
    "owner": "queuetime",
    "slug": "QTime",
    "version": "1.0.0",
    "plugins": [
      "@react-native-google-signin/google-signin",
      "expo-build-properties",
      "expo-apple-authentication",
      "@react-native-firebase/app",
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ]
    ],
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#1776ff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "usesAppleSignIn": true,
      "supportsTablet": false,
      "icon": "./assets/ios-icon.png",
      "bundleIdentifier": "com.capstone.QTime",
      "googleServicesFile": "./GoogleService-Info.plist",
      "infoPlist": {
        "UIBackgroundModes": ["location", "fetch"],
        "CFBundleVersion": "1.0.2",
        "NSLocationUsageDescription": "This app uses your location to locate the nearby point of interests and helps us compute an accurate wait time for point of interests.",
        "NSLocationAlwaysUsageDescription": "This app uses your background location to help us compute an accurate wait time for point of interests that you might be waiting in line for.",
        "NSLocationWhenInUseUsageDescription": "This app uses your location to locate the nearby point of interests and helps us compute an accurate wait time for point of interests."
      }
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "icon": "./assets/android-icon.png",
      "package": "com.capstone.QTime",
      "permissions": ["ACCESS_BACKGROUND_LOCATION", "ACCESS_FINE_LOCATION"],
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyCMNo6cmAGfp4OXo3LpWxSAWK-5Wu7_jys"
        }
      }
    },
    "extra": {
      "eas": {
        "projectId": "067cecb9-585a-415a-b692-afc88ed4aadd"
      }
    }
  }
}
