{
  "cli": {
    "version": ">= 3.1.1"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "development-ios": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": true,
        "resourceClass": "m1-medium"
      }
    },
    "preview": {
      "distribution": "internal",
      "extends": "production",
      "ios": {
        "resourceClass": "m1-medium"
      }
    },
    "production": {
      "env": {
        "BASE_URL": "https://queuetime.tech/api",
        "ENVIRONMENT": "prod"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
